<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.concrete5.org/doctrine-xml/0.5" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.concrete5.org/doctrine-xml/0.5 http://concrete5.github.io/doctrine-xml/doctrine-xml-0.5.xsd">
    <table name="btDummyImage">
        <field name="bID" type="integer">
            <unsigned/>
            <key/>
        </field>
        <field name="fg_color" type="string" size="10">
            <default value="white"/>
        </field>
        <field name="bg_color" type="string" size="10">
            <default value="grey"/>
        </field>
        <field name="img_text" type="text">
            <default value=""/>
        </field>
        <field name="img_width" type="integer" size="10">
            <default value="200"/>
        </field>
        <field name="img_height" type="integer" size="10">
            <default value="200"/>
        </field>
    </table>
</schema>
